<!DOCTYPE html>
<%- include('./header.ejs', {title: 'User view'}) %>
<%- include('./nav.ejs') %>

<main>
    <h1>user page</h1>
    <% Object.entries(allUsers).forEach(([key, value]) => { %>
        <p>name: <%= value.name %> </p>
        <p>age: <%= value.age %> </p>
        <p>gender: <%= value.gender %> </p>
        <p>lat: <%= value.locationLat %> </p>
        <p>long:</p><%= value.locationLong %> </p>
    <% }) %>
    </main>